{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "tsconfig.json",
    "tsconfig.base.json",
    ".eslintrc.cjs"
  ],
  "globalEnv": [
    "NODE_ENV",
    "VERCEL_ENV",
    "VERCEL_URL"
  ],
  "tasks": {
    "build": {
      "outputs": [
        "packages/**/dist/**",
        "packages/**/.next/**",
        "apps/**/.next/**",
        "dist/**",
        "**/.next/**",
        "**/.next-build-complete",
        "**/dist/**",
        "**/.build-complete",
        "apps/global/.next/**",
        "apps/journey/.next/**",
        "apps/machina/.next/**",
        "apps/motors/.next/**",
        "apps/outdoor/.next/**",
        "apps/publimicro/.next/**",
        "apps/publimicro/.next-build-complete",
        "apps/**/.next-build-complete",
        "apps/share/.next/**",
        "apps/tudo/.next/**",
        "apps/proper/.next/**"
      ]
    },
    "dev": {
      "cache": false
    }
  }
}